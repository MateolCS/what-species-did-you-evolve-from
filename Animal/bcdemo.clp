(defrule algae   ?p <- (pytanie (tresc "Do you eat french fries?"))   ?odp <- (odpowiedz (tresc "No"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Probably algae")	 (klucz "Brainiac")      )   ))(defrule pigeon   ?p <- (pytanie (tresc "Do you eat french fries?"))   ?odp <- (odpowiedz (tresc "Yes, and pretzels"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Feral pigeon")	 (klucz "Accident-prone")      )   ))(defrule pond   ?p <- (pytanie (tresc "In the shallows"))   ?odp <- (odpowiedz (tresc "Ponds & lakes"))   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "How's your buoyancy?")         (odpowiedzi "I sink like a rock" "I couldn't sink if I tried")      )   ))(defrule mussel   ?p <- (pytanie (tresc "How's your buoyancy?"))   ?odp <- (odpowiedz (tresc "I sink like a rock"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Thick shelled river mussel")	 (klucz "Brainiac")      )   ))(defrule goose   ?p <- (pytanie (tresc "How's your buoyancy?"))   ?odp <- (odpowiedz (tresc "I couldn't sink if I tried"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Lesser snow goose")	 (klucz "Flirt")      )   ))(defrule river   ?p <- (pytanie (tresc "In the shallows"))   ?odp <- (odpowiedz (tresc "Rivers & streams"))   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "Recreationally?")         (odpowiedzi "I like to goof around" "I can be pretty intense")      )   ))(defrule otter   ?p <- (pytanie (tresc "Recreationally?"))   ?odp <- (odpowiedz (tresc "I like to goof around"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Eurasion river otter")	 (klucz "Socialite")      )   ))(defrule pirhana   ?p <- (pytanie (tresc "Recreationally?"))   ?odp <- (odpowiedz (tresc "I can be pretty intense"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Red pirhana")	 (klucz "Anger issues")      )   ))(defrule deep-sea   ?p <- (pytanie (tresc "Where?"))   ?odp <- (odpowiedz (tresc "The deep blue sea"))   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "Alone?")         (odpowiedzi "Yes" "No")      )   ))(defrule alone   ?p <- (pytanie (tresc "Alone?"))   ?odp <- (odpowiedz (tresc "Yes"))   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "Bummer, why?")         (odpowiedzi "Because I'm mean" "Because I feel... invisible")      )   ))(defrule shark   ?p <- (pytanie (tresc "Bummer, why?"))   ?odp <- (odpowiedz (tresc "Because I'm mean"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Bull shark")	 (klucz "Anger issues")      )   ))(defrule jellyfish   ?p <- (pytanie (tresc "Bummer, why?"))   ?odp <- (odpowiedz (tresc "Because I feel... invisible"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Transparent jellyfish")	 (klucz "Rebel")      )   ))(defrule no-alone   ?p <- (pytanie (tresc "Alone?"))   ?odp <- (odpowiedz (tresc "No"))   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "With who?")         (odpowiedzi "My life mate" "All 15000 of my facebook friends")      )   ))(defrule angelfish   ?p <- (pytanie (tresc "With who?"))   ?odp <- (odpowiedz (tresc "My life mate"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "French angelfish")	 (klucz "Flirt")      )   ))(defrule barracuda   ?p <- (pytanie (tresc "With who?"))   ?odp <- (odpowiedz (tresc "All 15000 of my facebook friends"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Yellowtail barracuda")	 (klucz "Socialite")      )   ))(defrule the-deep-sea   ?p <- (pytanie (tresc "Where?"))   ?odp <- (odpowiedz (tresc "No, the DEEP blue sea"))   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "Are you scary?")         (odpowiedzi "Yes" "No")      )   ))(defrule scary   ?p <- (pytanie (tresc "Are you scary?"))   ?odp <- (or 	      (odpowiedz (tresc "No"))	      (odpowiedz (tresc "Yes"))	   )   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "Which do you have more of?")         (odpowiedzi "Teeth" "Appendages")      )   ))(defrule viperfish   ?p <- (pytanie (tresc "Which do you have more of?"))   ?odp <- (odpowiedz (tresc "Teeth"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Viperfish")	 (klucz "Anger issues")      )   ))(defrule squid   ?p <- (pytanie (tresc "Which do you have more of?"))   ?odp <- (odpowiedz (tresc "Appendages"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Giant squid")	 (klucz "Brainiac")      )   ))(defrule favorite-mate   ?p <- (pytanie (tresc "My favorite thing to do is:"))   ?odp <- (odpowiedz (tresc "Mate"))   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "Hit it & quit it?")         (odpowiedzi "Yes" "No")      )   ))(defrule dolphin   ?p <- (pytanie (tresc "Hit it & quit it?"))   ?odp <- (odpowiedz (tresc "Yes"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Bottlenose dolphin")	 (klucz "Flirt")      )   ))(defrule no-quit   ?p <- (pytanie (tresc "Hit it & quit it?"))   ?odp <- (odpowiedz (tresc "No"))   =>   (retract ?p)   (retract ?odp)   (assert      (pytanie         (tresc "Are you a stay at home dad?")         (odpowiedzi "Yes" "No")      )   ))(defrule seahorse   ?p <- (pytanie (tresc "Are you a stay at home dad?"))   ?odp <- (odpowiedz (tresc "Yes"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Pygmy seahorse")	 (klucz "Loaded")      )   ))(defrule dove   ?p <- (pytanie (tresc "Are you a stay at home dad?"))   ?odp <- (odpowiedz (tresc "No"))   =>   (retract ?p)   (retract ?odp)   (assert      (wynik         (zwierze "Turtle dove")	 (klucz "Celebrity")      )   ))